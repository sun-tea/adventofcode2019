{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "plugins": ["import"]
  },
  "rules": {
    "semi": 2,
    "no-console": "off",
    "arrow-parens": 1
  }
}
